@model List<BattlestationHub.Models.Setup>
@using Microsoft.AspNetCore.Identity

@inject UserManager<ApplicationUser> _userManager

@{
    ViewData["Title"] = "Profile";
}

<!-- Display user details and other information -->
@{
    if (Model.Count > 0)
    {
        var user = await _userManager.FindByIdAsync(Model[0].UserId);
        if (user != null)
        {
            <h1>Setups Uploaded by @user.DisplayName</h1>
        }
    }
}


@foreach (var userSetup in Model)
{
    <div>
        <a asp-action="Details" asp-route-id="@userSetup.Id">View</a>
        <h4>@userSetup.SetupName</h4>
        <p>@userSetup.SetupDesc</p>
    </div>
}
