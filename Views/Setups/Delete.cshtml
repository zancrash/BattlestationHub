@model BattlestationHub.Models.Setup

@{
    ViewData["Title"] = "Delete";
}

<div class="page-title-holder">
    <h1>Delete</h1>
    <hr />

</div>

<div class="page-content-holder" id="delete-holder">
    <h3>Are you sure you want to delete this setup?</h3>

    <img src="@("https://localhost:44384/" + Model.ImgPath)" alt="Card image cap">
    <h4>@Html.DisplayFor(model => model.SetupName)</h4>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> 
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
